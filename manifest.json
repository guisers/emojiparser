{
  "manifest_version": 2,
  "name": "Emoji Parser",
  "description": "Input emojis using the universal shortcodes anywhere in your browser",
  "version": "0.1.0",
  "icons": { 
    "128": "icon.png"
  },
  "permissions": [
    "http://*/*", 
    "https://*/*",
	"storage"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content_script.js"]
    }
  ]
}
